  "name": "scopesdf",
  "scripts": {
    "ds_store": "find . -name '.DS_Store' -type f -delete",
    "clean": "yarn ds_store && rimraf public/assets",
    "watch": "yarn clean && WEBPACK_CONFIG_TYPE='watch' webpack --progress --colors",
    "build": "yarn clean && WEBPACK_CONFIG_TYPE='build' webpack -p --progress --colors",
    "launch": "yarn clean && WEBPACK_CONFIG_TYPE='launch' webpack -p --progress --colors",
    "postinstall": "echo \"postinstall running\" && yarn build"
  },
